{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es6",
    "lib": [
      "es6",
      "webworker"
    ],
    "outDir": "../../frontend/js",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
